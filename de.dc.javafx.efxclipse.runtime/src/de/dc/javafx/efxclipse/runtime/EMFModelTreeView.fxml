<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.input.KeyCodeCombination?>
<?import javafx.scene.layout.VBox?>

<fx:root maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" stylesheets="@css/EMFModelView.css" type="VBox" xmlns="http://javafx.com/javafx/8.0.211" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TreeView fx:id="treeView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
         <contextMenu>
              <ContextMenu>
                <items>
                    <Menu fx:id="newMenu" mnemonicParsing="false" text="New">
                       <items>
                       </items>
                    </Menu>
                    <SeparatorMenuItem mnemonicParsing="false" />
                    <MenuItem mnemonicParsing="false" onAction="#onUndoMenuItemClicked" text="Undo">
                       <accelerator>
                          <KeyCodeCombination alt="UP" code="Z" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                       </accelerator>
                       <graphic>
                          <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                             <image>
                                <Image url="@icons/icons8-undo-16.png" />
                             </image>
                          </ImageView>
                       </graphic>
                    </MenuItem>
                    <MenuItem mnemonicParsing="false" onAction="#onRedoMenuItemClicked" text="Redo">
                       <accelerator>
                          <KeyCodeCombination alt="UP" code="Y" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                       </accelerator>
                       <graphic>
                          <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                             <image>
                                <Image url="@icons/icons8-redo-16.png" />
                             </image>
                          </ImageView>
                       </graphic>
                    </MenuItem>
                    <SeparatorMenuItem mnemonicParsing="false" />
                    <MenuItem mnemonicParsing="false" onAction="#onEditMenuItemClicked" text="Edit">
                       <graphic>
                          <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                             <image>
                                <Image url="@icons/icons8-edit-16.png" />
                             </image>
                          </ImageView>
                       </graphic>
                    </MenuItem>
                    <MenuItem mnemonicParsing="false" onAction="#onCopyMenuItemClicked" text="Copy">
                       <graphic>
                          <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                             <image>
                                <Image url="@icons/icons8-copy-to-clipboard-16.png" />
                             </image>
                          </ImageView>
                       </graphic>
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="C" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                    </MenuItem>
                    <MenuItem mnemonicParsing="false" onAction="#onPasteMenuItemClicked" text="Paste">
                       <graphic>
                          <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                             <image>
                                <Image url="@icons/icons8-paste-16.png" />
                             </image>
                          </ImageView>
                       </graphic>
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="V" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                    </MenuItem>
                    <SeparatorMenuItem mnemonicParsing="false" />
                    <MenuItem mnemonicParsing="false" onAction="#onDeleteMenuItemClicked" text="Delete">
                       <graphic>
                          <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                             <image>
                              <Image url="@icons/icons8-delete-16.png" />
                             </image>
                          </ImageView>
                       </graphic>
                      <accelerator>
                        <KeyCodeCombination alt="UP" code="DELETE" control="UP" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                    </MenuItem>
                </items>
              </ContextMenu>
           </contextMenu>
       </TreeView>
      <Label text="Model Path" textFill="WHITE">
         <padding>
            <Insets left="5.0" />
         </padding></Label>
   </children>
</fx:root>
