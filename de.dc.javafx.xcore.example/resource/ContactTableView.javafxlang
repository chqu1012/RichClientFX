packagePath "de.dc.javafx.xcore.example"

controls{
	TableViewFX{
		name: ContactTable 
		usedModel: de.dc.javafx.xcore.example.model.Contact
		column(name:Name) 
		column(name:Age cellValueFactory: AgeTreeCellFeature useFilter)
